        <div data-collapse="medium" data-animation="default" data-duration="400" data-easing="ease-out" role="banner" class="wrap cc-menu w-nav">
            <a href="@Url.Action("Index","Home")" aria-current="page" class="brand w-nav-brand w--current">
                <div class="text-block-2">HomeForHomeless</div>
            </a>
            <nav role="navigation" class="nav-menu w-nav-menu">
                <a href="@Url.Action("Index","Home")" aria-current="page" class="menu-item-wrap w-inline-block w--current">
                    <div class="menu-item">Home</div>
                    <div class="menu-item-deco-line"></div>
                </a>
                <a href="@Url.Action("VolunteerOrgan","Home")" class="menu-item-wrap w-inline-block">
                    <div class="menu-item">Volunteer</div>
                    <div class="menu-item-deco-line"></div>
                </a>
                <a href="@Url.Action("Index","freeServices")" class="menu-item-wrap w-inline-block">
                    <div class="menu-item">Free Services</div>
                    <div class="menu-item-deco-line"></div>
                </a>
                <a href="@Url.Action("Visualisation","Home")" class="menu-item-wrap w-inline-block">
                    <div class="menu-item">Learn More</div>
                    <div class="menu-item-deco-line"></div>
                </a>
                @*<a href="@Url.Action("Quiz","Home")" class="menu-item-wrap w-inline-block">
                    <div class="menu-item">quiz</div>
                    <div class="menu-item-deco-line"></div>
                </a>*@
            </nav>
            <div class="menu-button w-nav-button"><img src="~/images/Menu-Icon_1Menu-Icon.png" width="20" alt="" class="menu-icon"></div>
        </div>
    </div>

    <div>
        @RenderBody()
        <footer>
            <div data-w-id="2137cadc-f27c-f8b8-d4bf-ae8d48effbd1" class="content cc-subscribe-form">
                <div class="subscribe-wrap">
                    <div class="wrap cc-subscribe-form">
                        <div class="footer-subheadline">
                            <div class="subscribe-form-deco-line"></div>
                            <div class="label cc-subheadline w-inline-block">©Design By new Horizon</div>
                            <div class="subscribe-form-deco-line"></div>
                        </div>
                        <div class="subscribe-headline">HomeForHomeless</div>
                        <a href="https://homeforhomeless.azurewebsites.net/" target="_blank" style="color:white;">https://homeforhomeless.azurewebsites.net/ </a>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=5f4ccf2343c7b7d690e7d66d" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
