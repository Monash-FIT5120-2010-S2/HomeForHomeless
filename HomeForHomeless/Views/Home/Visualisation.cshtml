
@{
    ViewBag.Title = "Visualisation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="homelessinvicheadersection">
    <div class="slider-content">
        <div class="slide-content-wrap">
            <div class="container-2">
                <h1 data-w-id="65d0c8bb-f3cb-221f-3afc-7dc29700fdfe" class="headersectiontitle">Homelessness<br>‍<span class="brand-span">In Victoria</span></h1>
                <p data-w-id="65d0c8bb-f3cb-221f-3afc-7dc29700fe04" class="headersectionparagraph">Homelessness is a serious matter, but it is truly preventable.As proud citizens of Victoria, we can all make a difference.To help prevent homelessness we must first understand the underlying issue, its history, and the data behind it.</p>
            </div>
        </div>
    </div>
    <div class="slider-wrapper">
        <div data-delay="8000" data-animation="cross" data-autoplay="1" data-easing="ease-out" data-disable-swipe="1" data-duration="800" class="slider w-slider">
            <div class="w-slider-mask">
                <div class="slide w-slide">
                    <div class="homelessinvicheaderimage1"></div>
                </div>
                <div class="slide w-slide">
                    <div class="homelessinvicheaderimage2"></div>
                </div>
            </div>
            <div class="left-arrow-2 w-slider-arrow-left">
                <div class="w-icon-slider-left"></div>
            </div>
            <div class="right-arrow-2 w-slider-arrow-right">
                <div class="w-icon-slider-right"></div>
            </div>
            <div class="slide-nav-2 w-slider-nav w-round"></div>
        </div>
    </div>
</div>
<div data-w-id="00e39f18-964c-9533-8b86-2e9c9848835a" class="homepagesection">
    <div class="homepagesectioncontainer">
        <div class="homepagesectioncol w-row">
            <div class="homepagesectionimage w-col w-col-6">
                <div class="maincolorimage"><img src="~/images/support.jpg" height="500" srcset="~/images/support-p-500.jpeg 500w, ~/images/support.jpg 722w" sizes="(max-width: 479px) 450.4166564941406px, (max-width: 767px) 94vw, (max-width: 1439px) 47vw, 600px" data-w-id="056646df-faec-42fd-f6e3-4c0be7610260" alt="" class="image-7"></div>
            </div>
            <div class="homepagesectioncontent w-col w-col-6">
                <div class="homepagesectioncontentblock">
                    <h1 class="donatingsectiontitle">We can<br>‍<span class="homepagesectiontitlemaincolor">make differences</span></h1>
                    <p class="homepagesectionparagraph"> Currently, there are many misconceptions and discriminations against homeless individuals and many people are not aware of the reality of the situation. By spreading awareness of the facts, the statistics, and the truth, we hope to be able to create a more supportive and accommodating environment to help the homeless get through a temporary bump in their lives.</p>
                </div>
            </div>
        </div>
    </div>
</div>
<div data-w-id="d63a3ebc-2aa2-ea39-81be-0c884064eb29" class="spacesection">
    <div class="container-flex-2">
        <div class="providesupportcontainer"></div>
    </div>
</div>
<div data-w-id="83f4bdb5-ed76-edbb-5756-fd83ff4a4535" style="opacity:0" class="homepagesection">
    <div class="homepagesectioncontainer">
        <div class="dataanalysistitlecontent">
            <h1 class="homelessinvictitle">Data<span class="brand-span"> Analysis</span></h1>
            <p class="homelessinvicdes"> Together, we can and will end homelessness in Victoria..</p>
        </div>
        <div class="w-layout-grid dataanalysiscontent">
            <div class="analysis">
                <img src="~/images/dataVir.png" alt="" class="grid-image">
                <h6 class="dataanalysistitle">Homeless population in age group</h6>
                <p class="paragraph-13">Age distribution of Homeless in Victoria and how they have changed.</p>
                <a href="@Url.Action("VisualisationDetailsAgeGroup", "Home")" class="dataanalysisbutton w-button">Show It</a>
            </div>
            <div class="analysis">
                <img src="~/images/dataVir.png" alt="" class="grid-image">
                <h6 class="dataanalysistitle">Growth of homelessness population<br></h6>
                <p class="paragraph-13">How the numbers of homelessness have changed over the years in Victoria.</p>
                <a href="@Url.Action("VisualisationDetailsNumberByYear", "Home")" class="dataanalysisbutton w-button">Show It</a>
            </div>
            <div class="analysis">
                <img src="~/images/dataVir.png" alt="" class="grid-image">
                <h6 class="dataanalysistitle">Homeless population<br>by suburb</h6>
                <p class="paragraph-13">The situation of homeless spread across Victoria.</p>
                <a href="@Url.Action("VisualisationDetailsSuburb", "Home")" class="dataanalysisbutton w-button">Show It</a>
            </div>
            <div class="analysis">
                <img src="~/images/dataVir.png" alt="" class="grid-image">
                <div class="dataanalysistitlewrap">
                    <h6 class="dataanalysistitle">Homelessness population accommodation information</h6>
                </div>
                <p class="paragraph-13">The information about where do the homeless individual stay in Victoria.</p>
                <a href="@Url.Action("VisualisationDetailsOperationGroups", "Home")" class="dataanalysisbutton w-button">Show It</a>
            </div>
            <div class="analysis">
                <img src="~/images/dataVir.png" alt="" class="grid-image">
                <div class="dataanalysistitlewrap">
                    <h6 class="dataanalysistitle">Homeless population by different states in Victoria</h6>
                </div>
                <p class="paragraph-13">The information about homelessness population in different states of Victoria.</p>
                <a href="@Url.Action("VisualisationDetailsState", "Home")" class="dataanalysisbutton w-button">Show It</a>
            </div>
            <div class="analysis">
                <img src="~/images/dataVir.png" alt="" class="grid-image">
                <div class="dataanalysistitlewrap">
                    <h6 class="dataanalysistitle">Homeless population gender in Victoria</h6>
                </div>
                <p class="paragraph-13">Statistics of gender in homeless individuals in Victoria</p>
                <a href="@Url.Action("VisualisationGender", "Home")" class="dataanalysisbutton w-button">Show It</a>
            </div>
        </div>
    </div>
</div>


<script>
    window.onload = function ChangeNavigationColor() {
        // Get all the matching elements into a JavaScript Array
        var elements = Array.prototype.slice.call(document.querySelectorAll(".navigation-text"));
        // Loop over each element....
        elements.forEach(function (el) {
            el.style.color = "hsl(39deg 18% 81%)";  // Adjust the element's style
        }); 
        var navi = document.getElementById("navigation-bar");
        navi.style.background = "rgba(105,105,105,0.8)";
        document.getElementById("navi-logo-img").src = '/images/yellowLogo.png';
    }
</script>

