<script>
    window.onload = function changeNavigationBarBkgrdColor() {
        // Get all the matching elements into a JavaScript Array
        var elements = Array.prototype.slice.call(document.querySelectorAll(".navigation-text"));

        // Loop over each element....
        elements.forEach(function (el) {
            el.style.color = "#dcd6ca";  // Adjust the element's style
        });
        document.getElementById("navigation-bar").style.background_color = "hsl(0deg 0% 20%)";
        document.getElementById("navigation-bar").style.opacity = "0.7";
        document.getElementById("navi-logo-img").src = '/images/yellowLogo.png';
    }
</script>
